    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "request-ip": "^2.1.3",
    "sequelize": "^6.12.0-alpha.1",
    "sqlite3": "^5.0.2",
    "tailwindcss": "^2.2.19",
    "use-utility-classes": "^1.2.1"
  },
  "devDependencies": {
